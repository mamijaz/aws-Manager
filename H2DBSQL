CREATE TABLE USERS(USERNAME VARCHAR(50),PASSWORD VARCHAR(50) NOT NULL,USERTYPE VARCHAR(50),PRIMARY KEY(USERNAME));
CREATE TABLE SERVERS(INSTANCEID VARCHAR(50),REGION VARCHAR(50) NOT NULL,NAME VARCHAR(100),PRIMARY KEY(INSTANCEID));
CREATE TABLE USER_SERVERS(USERNAME VARCHAR(50),INSTANCEID VARCHAR(50),FOREIGN KEY(USERNAME ) REFERENCES USERS(USERNAME ),FOREIGN KEY(INSTANCEID) REFERENCES SERVERS(INSTANCEID),PRIMARY KEY(USERNAME , INSTANCEID ));
INSERT INTO USERS VALUES('admin','admin','admin');
INSERT INTO USERS VALUES('user','user','user');
